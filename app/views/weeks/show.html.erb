<% provide(:title, "week #{@week.week_num}") %>
<div class="row">
  <aside class="span4">
    <section>
      <h1>
        week <%= @week.week_num %>
      </h1>
    </section>
  </aside>
  <div class="span8">
    <% if @games.any? %>
      <h3>Games (<%= @games.count %>)</h3>
      <ol class="games">
        <%= render @games %>
      </ol>
    <% end %>
  </div>
  <div>
    <%= link_to "Entry", user_entry_path(@user, @entry) %>
    <%= link_to "Result", result_week_path %>
    <%= link_to "What if...", what_if_week_path %>
  </div>
</div>
